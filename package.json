{
  "name": "dotnet-settings",
  "version": "0.1.0",
  "description": "A Node Module to easily manage Dotnet Core Project Settings per Configuration",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@git.x-company.de:global/dotnet-settings.git"
  },
  "bugs": {
    "url": "https://github.com/x-company/dotnet-settings/issues"
  },
  "homepage": "https://github.com/x-company/dotnet-settings",
  "author": "Roland Breitschaft <roland.breitschaft@x-company.de>",
  "license": "MIT",
  "keywords": [
    "build",
    "settings",
    "dotnet",
    ".Net Core",
    ".Net",
    "DotNet",
    "DotNet Core",
    "teambuild",
    "tfs",
    "teamfoundation"
  ],
  "scripts": {
    "lint": "tslint -c ./tslint.json './src/**/*.ts'",
    "prebuild": "yarn lint",
    "build": "tsc -p ./src/tsconfig.json",
    "postbuild": "appvmgr update build",
    "watch": "tsc -w -p ./src/tsconfig.json",
    "pretest": "yarn build",
    "test": "jasmine-ts 'spec/**/*.spec.ts'",
    "prepublish": "yarn test && appvmgr update commit --tag"
  },
  "dependencies": {
    "appversion-mgr": "^0.7.0",
    "find-root": "^1.1.0",
    "fs-extra": "^7.0.1",
    "get-caller-file": "^2.0.1",
    "log-symbols": "^2.2.0",
    "node-fetch": "^2.3.0",
    "semver": "^5.6.0",
    "xml-js": "^1.6.8"
  },
  "devDependencies": {
    "@types/find-root": "^1.1.1",
    "@types/fs-extra": "^5.0.4",
    "@types/get-caller-file": "^1.0.0",
    "@types/jasmine": "^3.3.5",
    "@types/log-symbols": "^2.0.0",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "@types/semver": "^5.5.0",
    "@types/xml-js": "^1.0.0",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
