{
    "licenser.projectName": "dotnet-settings",
    "licenser.license": "MIT",
    "licenser.author": "IT Solutions Roland Breitschaft <info@x-company.de>",
    "licenser.disableAutoHeaderInsertion": false,
    "licenser.useSingleLineStyle": false,
    "codeheader.copyright": false,
    "codeheader.autoInsert": true,
    "jasmineExplorer.nodeArgv": [
        "--require",
        "ts-node/register"
    ],
    "jasmineExplorer.env": {
        "TS_NODE_PROJECT" : "./src/tsconfig.json"
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/changelogs": true,
        "**/doc": true,
        "**/.vscode": true,
        "**/.gitlab": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/*.d.ts": true,
        "node_modules": true,
        "**/package-lock.json": true,
        "**/yarn.lock": true
    },
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/node_modules/*/**": true,
        "**/.gitlab/*/**": true,
        "**/.vscode/*/**": true
    },
    "tslint.alwaysShowStatus": true,
    "tslint.configFile": "./tslint.json",
    "tslint.jsEnable": false,
    "tslint.packageManager": "yarn",
    "tslint.run": "onType",
    "todo-tree.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.excludeGlobs": [
        "node_modules/**/*"
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.filtering.excludeGlobs": [
        "node_modules/**/*"
    ]
}
